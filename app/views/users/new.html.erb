<h2>Sign Up</h2>
<form action='<%= users_url %>' method='post' class='sign-up-form'>
	<%= auth_token %>
	<label>Username:
		<input type='text' name='user[username]'>
	</label><br>
	<label>Password:
		<input type='password' name='user[password]'>
	</label><br>
	<input type='submit' value='Sign Up' class='sign-up-btn'>
</form>
