<h2>Log In</h2>
<form action='<%= session_url %>' method='post'>
	<%= auth_token %>
	<label>Username: 
		<input type='text' name='user[username]'>
	</label><br>
	<label>Password: 
		<input type='password' name='user[password]'>
	</label><br>
	<input type='submit' value='Log In' class='log-in-btn'>
</form>